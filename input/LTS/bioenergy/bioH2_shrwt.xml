                    <share-weight fillout="1" year="2030">0.4</share-weight>
                    <share-weight fillout="1" year="2035">0.6</share-weight>
                    <share-weight fillout="1" year="2040">0.8</share-weight>
                    <share-weight fillout="1" year="2045">1</share-weight>
                    <share-weight fillout="1" year="2050">1</share-weight>
                </subsector>
            </supplysector>
            
        </region>
   </world>
</scenario>